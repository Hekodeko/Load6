-- Script for Delta Executor
-- UI for teleporting or flinging players in Brookhaven RP

-- Create Screen GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "MainUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = game.CoreGui

-- Create Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 400, 0, 200)
MainFrame.Position = UDim2.new(0.5, -200, 0.5, -100)
MainFrame.BackgroundColor3 = Color3.fromRGB(128, 128, 128)  -- Gray Color
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

-- Title Label
local Title = Instance.new("TextLabel")
Title.Text = "Brookhaven Player Control"
Title.Size = UDim2.new(1, 0, 0, 50)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black Color
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 24
Title.Parent = MainFrame

-- Close Button
local CloseButton = Instance.new("TextButton")
CloseButton.Text = "X"
CloseButton.Size = UDim2.new(0, 50, 0, 50)
CloseButton.Position = UDim2.new(1, -50, 0, 0)
CloseButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black Color
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
CloseButton.Parent = MainFrame

-- Close functionality
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()  -- Close the entire UI
end)

-- Player Name Input Box
local PlayerInput = Instance.new("TextBox")
PlayerInput.Size = UDim2.new(1, -20, 0, 50)
PlayerInput.Position = UDim2.new(0, 10, 0, 60)
PlayerInput.PlaceholderText = "Enter player name"
PlayerInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
PlayerInput.TextColor3 = Color3.fromRGB(0, 0, 0)
PlayerInput.Parent = MainFrame

-- Fling Button
local FlingButton = Instance.new("TextButton")
FlingButton.Text = "Fling Player"
FlingButton.Size = UDim2.new(0.45, 0, 0, 50)
FlingButton.Position = UDim2.new(0, 10, 0, 120)
FlingButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black Color
FlingButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FlingButton.Parent = MainFrame

-- Teleport Button
local TeleportButton = Instance.new("TextButton")
TeleportButton.Text = "Teleport Player"
TeleportButton.Size = UDim2.new(0.45, 0, 0, 50)
TeleportButton.Position = UDim2.new(0.55, 0, 0, 120)
TeleportButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black Color
TeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TeleportButton.Parent = MainFrame

-- Made by Label with clickable link
local MadeByLabel = Instance.new("TextButton")
MadeByLabel.Text = "Made by ismaha08"
MadeByLabel.Size = UDim2.new(1, -20, 0, 30)
MadeByLabel.Position = UDim2.new(0, 10, 0, 180)
MadeByLabel.BackgroundTransparency = 1
MadeByLabel.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black Text
MadeByLabel.Font = Enum.Font.SourceSansBold
MadeByLabel.TextSize = 16
MadeByLabel.Parent = MainFrame

MadeByLabel.MouseButton1Click:Connect(function()
    setclipboard("https://www.roblox.com/users/5509803778/profile?friendshipSourceType=PlayerSearch")  -- Copies link to clipboard
end)

-- Fling functionality
FlingButton.MouseButton1Click:Connect(function()
    local playerName = PlayerInput.Text
    local targetPlayer = game.Players:FindFirstChild(playerName)
    if targetPlayer then
        -- Fling logic here
        local character = targetPlayer.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.Velocity = Vector3.new(0, 1000, 0) -- Fling upwards
        end
    else
        warn("Player not found!")
    end
end)

-- Teleport functionality
TeleportButton.MouseButton1Click:Connect(function()
    local playerName = PlayerInput.Text
    local targetPlayer = game.Players:FindFirstChild(playerName)
    if targetPlayer then
        -- Teleport logic here
        local localPlayer = game.Players.LocalPlayer
        local targetCharacter = targetPlayer.Character
        if localPlayer.Character and targetCharacter then
            localPlayer.Character:SetPrimaryPartCFrame(targetCharacter.PrimaryPart.CFrame) -- Teleport
        end
    else
        warn("Player not found!")
    end
end)

-- Execution instructions
print("Script executed. Use UI to control players.")
